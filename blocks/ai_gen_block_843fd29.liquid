{% doc %}
  @prompt
    I want image below text with url section  
    show more button, i want after 12 blocks

{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-image-text-grid-{{ ai_gen_id }} {
    padding: {{ block.settings.section_padding }}px 0;
  }

  .ai-image-text-container-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat({{ block.settings.columns_desktop }}, 1fr);
    gap: {{ block.settings.grid_gap }}px;
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 20px;
  }

  .ai-image-text-item-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column-reverse;
    text-align: {{ block.settings.text_alignment }};
    transition: transform 0.3s ease;
  }

  .ai-image-text-item-{{ ai_gen_id }}:hover {
    transform: translateY(-5px);
  }

  .ai-image-text-content-{{ ai_gen_id }} {
    order: 1;
    margin-bottom: {{ block.settings.content_spacing }}px;
  }

  .ai-image-text-image-wrapper-{{ ai_gen_id }} {
    order: 2;
    width: 100%;
    aspect-ratio: {{ block.settings.image_aspect_ratio }};
    overflow: hidden;
    border-radius: {{ block.settings.image_border_radius }}px;
  }

  .ai-image-text-image-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: transform 0.3s ease;
  }

  .ai-image-text-item-{{ ai_gen_id }}:hover .ai-image-text-image-{{ ai_gen_id }} {
    transform: scale(1.05);
  }

  .ai-image-text-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    background-color: #f4f4f4;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-image-text-placeholder-{{ ai_gen_id }} svg {
    width: 60%;
    height: 60%;
    opacity: 0.3;
  }

  .ai-image-text-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.title_size }}px;
    font-weight: {{ block.settings.title_weight }};
    color: {{ block.settings.title_color }};
    margin: 0 0 {{ block.settings.title_margin }}px 0;
    line-height: 1.3;
  }

  .ai-image-text-description-{{ ai_gen_id }} {
    font-size: {{ block.settings.description_size }}px;
    color: {{ block.settings.description_color }};
    line-height: 1.5;
    margin: 0;
  }

  .ai-image-text-link-{{ ai_gen_id }} {
    text-decoration: none;
    color: inherit;
    display: block;
  }

  .ai-image-text-show-more-{{ ai_gen_id }} {
    text-align: center;
    margin-top: {{ block.settings.button_margin }}px;
  }

  .ai-image-text-show-more-button-{{ ai_gen_id }} {
    background-color: {{ block.settings.button_bg_color }};
    color: {{ block.settings.button_text_color }};
    border: {{ block.settings.button_border_width }}px solid {{ block.settings.button_border_color }};
    padding: {{ block.settings.button_padding }}px {{ block.settings.button_padding | times: 1.5 }}px;
    border-radius: {{ block.settings.button_border_radius }}px;
    font-size: {{ block.settings.button_font_size }}px;
    cursor: pointer;
    transition: all 0.3s ease;
  }

  .ai-image-text-show-more-button-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.button_hover_bg_color }};
    color: {{ block.settings.button_hover_text_color }};
    border-color: {{ block.settings.button_hover_border_color }};
  }

  .ai-image-text-item-{{ ai_gen_id }}.hidden {
    display: none;
  }

  @media screen and (max-width: 768px) {
    .ai-image-text-container-{{ ai_gen_id }} {
      grid-template-columns: repeat({{ block.settings.columns_mobile }}, 1fr);gap: {{ block.settings.grid_gap | times: 0.75 }}px;
      padding: 0 15px;
    }

    .ai-image-text-title-{{ ai_gen_id }} {
      font-size: {{ block.settings.title_size | times: 0.9 }}px;
    }

    .ai-image-text-description-{{ ai_gen_id }} {
      font-size: {{ block.settings.description_size | times: 0.9 }}px;
    }
  }
{% endstyle %}

<image-text-grid-{{ ai_gen_id }}
  class="ai-image-text-grid-{{ ai_gen_id }}"
  {{ block.shopify_attributes }}
  data-initial-count="12"
>
  <div class="ai-image-text-container-{{ ai_gen_id }}">
    {% for i in (1..24) %}
      {% liquid
        assign item_enabled_key = 'item_' | append: i | append: '_enabled'
        assign item_title_key = 'item_' | append: i | append: '_title'
        assign item_description_key = 'item_' | append: i | append: '_description'
        assign item_image_key = 'item_' | append: i | append: '_image'
        assign item_url_key = 'item_' | append: i | append: '_url'

        assign item_enabled = block.settings[item_enabled_key]
        assign item_title = block.settings[item_title_key]
        assign item_description = block.settings[item_description_key]
        assign item_image = block.settings[item_image_key]
        assign item_url = block.settings[item_url_key]
      %}

      {% if item_enabled %}
        {% assign item_class = 'ai-image-text-item-' | append: ai_gen_id %}
        {% if i > 12 %}
          {% assign item_class = item_class | append: ' hidden' %}
        {% endif %}

        <div class="{{ item_class }}" data-item-index="{{ i }}">



        <div class="ai-image-text-image-wrapper-{{ ai_gen_id }}">
            {% if item_image %}
              <img
                src="{{ item_image | image_url: width: 600 }}"
                alt="{{ item_image.alt | escape }}"
                class="ai-image-text-image-{{ ai_gen_id }}"
                loading="lazy"
                width="{{ item_image.width }}"
                height="{{ item_image.height }}"
              >
            {% else %}
              <div class="ai-image-text-placeholder-{{ ai_gen_id }}">
                {{ 'image' | placeholder_svg_tag }}
              </div>
            {% endif %}
          </div>


          {% if item_url != blank %}
            <a href="{{ item_url }}" class="ai-image-text-link-{{ ai_gen_id }}">
          {% endif %}<div class="ai-image-text-content-{{ ai_gen_id }}">
            {% if item_title != blank %}
              <h3 class="ai-image-text-title-{{ ai_gen_id }}">{{ item_title }}</h3>
            {% endif %}
            {% if item_description != blank %}
              <p class="ai-image-text-description-{{ ai_gen_id }}">{{ item_description }}</p>
            {% endif %}
          </div>

          {% if item_url != blank %}
            </a>
          {% endif %}
        </div>
      {% endif %}
    {% endfor %}
  </div>

  <div class="ai-image-text-show-more-{{ ai_gen_id }}" style="display: none;">
    <button class="ai-image-text-show-more-button-{{ ai_gen_id }}" type="button">
      {{ block.settings.show_more_text }}
    </button>
  </div>
</image-text-grid-{{ ai_gen_id }}>

<script>
  (function() {
    class ImageTextGrid{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.initialCount = parseInt(this.dataset.initialCount) || 12;
        this.currentCount = this.initialCount;
      }

      connectedCallback() {
        this.items = this.querySelectorAll('.ai-image-text-item-{{ ai_gen_id }}');
        this.showMoreContainer = this.querySelector('.ai-image-text-show-more-{{ ai_gen_id }}');
        this.showMoreButton = this.querySelector('.ai-image-text-show-more-button-{{ ai_gen_id }}');
        this.setupShowMore();
      }

      setupShowMore() {
        const totalEnabledItems = this.items.length;
        if (totalEnabledItems > this.initialCount) {
          this.showMoreContainer.style.display = 'block';
          this.showMoreButton.addEventListener('click', () => {
            this.showMoreItems();
          });
        }
      }

      showMoreItems() {
        const hiddenItems = Array.from(this.items).filter(item => 
          item.classList.contains('hidden')
        );
        
        const itemsToShow = hiddenItems.slice(0, this.initialCount);
        
        itemsToShow.forEach(item => {
          item.classList.remove('hidden');
        });
        
        this.currentCount += itemsToShow.length;
        
        if (hiddenItems.length <= this.initialCount) {
          this.showMoreContainer.style.display = 'none';
        }
      }
    }

    customElements.define('image-text-grid-{{ ai_gen_id }}', ImageTextGrid{{ ai_gen_id }});
  })();
</script>

